@inject NetCoreMVC.Services.PlanetService planetService
<h1>Các hành tinh</h1>


<div class="list-group">
    @foreach (var planet in planetService)
    {
        <a class="list-group-item list-group-item-action" asp-action="@planet.Name">
            @planet.Vietnamese_name - @planet.Name
        </a>
    }
</div>

<p>@Url.RouteUrl("neptune1")</p>
